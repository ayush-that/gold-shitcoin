    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "format": "prettier --write \"**/*.{js,json,css,md}\""
  },
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "ethers": "^6.13.4",
    "express": "^4.19.2",
    "node-cron": "^3.0.3",
    "prettier": "^3.6.2"
  },
  "keywords": [
    "bsc",
    "token",
    "holders",
    "bscscan"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "supertest": "^7.1.4",
    "vitest": "^3.2.4"
  }
