{
  "name": "bsc-top-holders",
  "version": "1.0.0",
  "description": "Get top 100 holders of a BSC token",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "bridge:test": "node bridge-test.js",
    "run:once": "node -e \"import('./orchestrator.js').then(m => m.runDistributionCycle().then(r => console.log(JSON.stringify(r, null, 2))))\"",
